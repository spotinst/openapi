{
  "name": "spot-openapi",
  "description": "Spot OpenAPI",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/spotinst/spotinst-openapi"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Spot by NetApp",
    "url": "https://spot.io"
  },
  "scripts": {
    "serve": "redoc-cli serve --options.sortPropsAlphabetically --options.menuToggle --options.expandSingleSchemaField --watch api/spot.yaml",
    "bundle": "redoc-cli bundle --options.sortPropsAlphabetically --options.menuToggle --options.expandSingleSchemaField --output build/index.html api/spot.yaml",
    "bundle:json": "swagger-cli bundle --outfile build/bundle.json api/spot.yaml",
    "validate": "swagger-cli validate --no-schema api/spot.yaml",
    "clean": "rm -rf build"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "^4.0.4",
    "redoc-cli": "^0.9.12"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "swagger-cli": "^4.0.4"
  }
}
