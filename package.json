{
  "name": "openapi",
  "description": "Spot OpenAPI",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/spotinst/openapi"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Spot by NetApp",
    "url": "https://spot.io"
  },
  "scripts": {
    "build:administration": "redoc-cli build --template redoc/template.hbs.html --options.menuToggle=true,--options.expandSingleSchemaField=true,--options.sortPropsAlphabetically=true,--options.generatedPayloadSamplesMaxDepth=20,--options.theme.colors.primary.main=#0086ff --output build/administration.html api/sections/administration.yaml",
    "build:elastigroup": "redoc-cli build --template redoc/template.hbs.html --options.menuToggle=true,--options.expandSingleSchemaField=true,--options.sortPropsAlphabetically=true,--options.generatedPayloadSamplesMaxDepth=20,--options.theme.colors.primary.main=#0086ff --output build/elastigroup.html api/sections/elastigroup.yaml",
    "build:ocean": "redoc-cli build --template redoc/template.hbs.html --options.menuToggle=true,--options.expandSingleSchemaField=true,--options.sortPropsAlphabetically=true,--options.generatedPayloadSamplesMaxDepth=20,--options.theme.colors.primary.main=#0086ff --output build/ocean.html api/sections/ocean.yaml",
    "build:managedInstance": "redoc-cli build --template redoc/template.hbs.html --options.menuToggle=true,--options.expandSingleSchemaField=true,--options.sortPropsAlphabetically=true,--options.generatedPayloadSamplesMaxDepth=20,--options.theme.colors.primary.main=#0086ff --output build/managedInstance.html api/sections/managedInstance.yaml",
    "build:index": "redoc-cli build --template redoc/template.hbs.html --options.menuToggle=true,--options.expandSingleSchemaField=true,--options.sortPropsAlphabetically=true,--options.generatedPayloadSamplesMaxDepth=20,--options.theme.colors.primary.main=#0086ff --output build/index_new.html api/sections/index_new.yaml",
    "build:services": "redoc-cli build --template redoc/template.hbs.html --options.menuToggle=true,--options.expandSingleSchemaField=true,--options.sortPropsAlphabetically=true,--options.generatedPayloadSamplesMaxDepth=20,--options.theme.colors.primary.main=#0086ff --output build/services.html api/sections/services.yaml",
    "serve": "redoc-cli serve --template redoc/template.hbs.html --options='{\"menuToggle\":true,\"expandSingleSchemaField\":true,\"sortPropsAlphabetically\":true,\"generatedPayloadSamplesMaxDepth\":20,\"theme\":{\"colors\":{\"primary\":{\"main\":\"#0086ff\"}}}}' --watch api/spot.yaml",
    "bundle:redoc": "yarn run build:administration && yarn run build:elastigroup && yarn run build:ocean && yarn run build:managedInstance && yarn run build:services && yarn run build:index",
    "bundle:swagger": "swagger-cli bundle --outfile build/openapi.json api/spot.yaml",
    "validate": "swagger-cli validate --no-schema api/spot.yaml",
    "clean": "rm -rf build"
  },
  "devDependencies": {
    "prettier": "^2.6.2",
    "redoc-cli": "^0.13.10",
    "swagger-cli": "^4.0.4"
  },
  "engines": {
    "node": ">=12"
  }
}
